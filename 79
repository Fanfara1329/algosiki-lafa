#include <iostream>
#include <numeric>
#include <vector>

using namespace std;

int main() {
    int n, x;
    cin >> n >> x;
    vector<int> v(n);
    for (auto &i: v) cin >> i;
    int l = 0, r = n - 1;
    while (r != l) {
        int mid = (r + l) / 2;
        if (v[mid] < x) {
            l = mid + 1;
        } else {
            r = mid;
        }
        //cout << l << " " << mid << " " << r << endl;
    }
    //cout << "XXXXXXXXXXXX\n";
    int ans = l;
    l = 0, r = n - 1;
    while (l != r) {
        int mid = (r + l + 1) / 2; // mid = (r + l + 1) / 2
        if (v[mid] > x) r = mid -1;
        else l = mid;
        //cout << l << " " << mid << " " << r << endl;
    }
    cout << l - ans + 1;
    return 0;
}

//10 4
//1 2 3 3 4l 4 4r 6 7 10
